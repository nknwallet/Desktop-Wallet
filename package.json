{
  "name": "nkn-wallet",
  "version": "1.0.0",
  "description": "NKN Wallet",
  "main": "build/electron.js",
  "scripts": {
    "start": "electron .",
    "dev:build": "cross-env NODE_ENV=development ./node_modules/.bin/gulp dev",
    "prod:build": "cross-env NODE_ENV=production ./node_modules/.bin/gulp prod",
    "package:linux": "./node_modules/.bin/electron-packager ./ nkn-wallet --platform=linux --asar --arch=x64 --icon=./src/images/icons/256x256.png --out=dist --overwrite",
    "package:macos": "./node_modules/.bin/electron-packager ./ nkn-wallet --platform=darwin --asar --arch=x64 --icon=./src/images/icons/256x256.icns --out=dist --overwrite",
    "package:windows": "./node_modules/.bin/electron-packager ./ nkn-wallet --platform=win32 --asar --arch=x64 --icon=./src/images/icons/256x256.ico --out=dist --overwrite",
    "build:deb": "./node_modules/.bin/electron-installer-debian --arch amd64 --config ./packager/deb.json",
    "build:dmg": "./node_modules/.bin/electron-installer-dmg ./dist/nkn-wallet-darwin-x64/nkn-wallet.app nkn-wallet --icon=./src/images/icons/256x256.icns --out=./dist/installers/ --title=nkn-wallet --overwrite",
    "build:rpm": "./node_modules/.bin/electron-installer-redhat --src ./dist/nkn-wallet-linux-x64 --arch x86_64 --config ./packager/rpm.json",
    "build:flatpak": "./node_modules/.bin/electron-installer-flatpak --src ./dist/nkn-wallet-linux-x64 --arch x86_64 --config ./packager/flatpak.json",
    "build:snap": "./node_modules/.bin/electron-installer-snap --src=./dist/nkn-wallet-linux-x64 --name nkn-wallet --dest=./dist/installer",
    "release:deb": "npm run package:linux; npm run build:deb",
    "release:dmg": "npm run package:macos; npm run build:dmg",
    "release:rpm": "npm run package:linux; npm run build:rpm",
    "release:flatpak": "npm run package:linux; npm run build:flatpak",
    "release:snap": "npm run package:linux; npm run build:snap"
  },
  "repository": "https://github.com/matinkaboli/wallet",
  "keywords": [
    "NKN",
    "Wallet"
  ],
  "author": "Matin Kaboli <matinkaboli@aol.com>",
  "license": "CC0-1.0",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "chart.js": "^2.7.3",
    "classnames": "^2.2.6",
    "electron": "^4.0.1",
    "fs-extra": "^7.0.1",
    "history": "^4.7.2",
    "moment": "^2.24.0",
    "nkn-wallet": "^0.2.0",
    "node-fetch": "^2.3.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-chartkick": "^0.3.0",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "rodal": "^1.6.3",
    "shortid": "^2.2.14",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "uuid": "^3.3.2",
    "webpack": "^4.25.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "del": "^3.0.0",
    "electron-installer-debian": "^1.1.0",
    "electron-installer-dmg": "^2.0.0",
    "electron-installer-flatpak": "^0.8.0",
    "electron-installer-redhat": "^1.0.1",
    "electron-installer-snap": "^3.1.1",
    "electron-packager": "^13.0.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-eslint": "^5.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2"
  }
}
